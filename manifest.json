{
  "manifest_version": 2,
 
  "name": "Modify CSS and HTML",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",
    
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["<all_urls>"]
  }],
    "browser_action": { 
      "default_popup" : "popup.html"
},
    "permissions": [ 
        "tabs"
    ]
}